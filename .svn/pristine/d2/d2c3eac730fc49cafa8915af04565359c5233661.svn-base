        </activity>
        <activity
            android:name="br.com.simdata.forcavendas.view.CfgActivity"
            android:label="Configuracao" >
        </activity>
        <activity
            android:name="br.com.simdata.forcavendas.view.ListClientesActivity"
            android:label="Lista Clientes" >
        </activity>
        <activity
            android:name="br.com.simdata.forcavendas.view.FormClientesActivity"
            android:label="Cliente" >
        </activity>
