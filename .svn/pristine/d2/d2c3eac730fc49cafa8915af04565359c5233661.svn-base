        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="br.com.simdata.forcavendas.view.SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.simdata.forcavendas.view.MainActivity"
            android:label="SimDataForcaVendas" >
        </activity>
        <activity
            android:name="br.com.simdata.forcavendas.view.FormLoginActivity"
