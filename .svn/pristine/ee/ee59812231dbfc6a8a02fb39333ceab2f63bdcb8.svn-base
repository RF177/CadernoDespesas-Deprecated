package br.com.simdata.forcavendas.model.venda;

public class PedidoItem {

	private int id;
	private long id_pedido;
	private long id_produto;
	private double qtd;
	private double vl_unitario;
	private double descp;
	private double total;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public long getId_pedido() {
		return id_pedido;
	}

	public void setId_pedido(long id_pedido) {
		this.id_pedido = id_pedido;
	}

	public long getId_produto() {
		return id_produto;
	}

	public void setId_produto(long id_produto) {
		this.id_produto = id_produto;
	}

	public double getQtd() {
		return qtd;
	}

	public void setQtd(double qtd) {
		this.qtd = qtd;
	}

	public double getVl_unitario() {
		return vl_unitario;
	}

	public void setVl_unitario(double vl_unitario) {
		this.vl_unitario = vl_unitario;
	}

	public double getDescp() {
		return descp;
	}

	public void setDescp(double descp) {
		this.descp = descp;
	}

	public double getTotal() {
		return total;
	}

	public void setTotal(double total) {
		this.total = total;
	}
}